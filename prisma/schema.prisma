generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Client {
  id           Int     @id @default(autoincrement())
  key          String  @unique @default(uuid())
  cpf          String?
  name         String?
  email        String?
  is_anonymous Boolean

  @@map("clients")
}

